{
	"name": "oc2dart",
	"displayName": "",
	"publisher": "bung87",
	"description": "convert Objective-C header to dart",
	"version": "0.0.4",
	"engines": {
		"vscode": "^1.43.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/bung87/oc2dart-vscode-extension.git"
	},
	"galleryBanner": {
		"color": "#60caf6",
		"theme": "light"
	},
	"activationEvents": [
		"onCommand:extension.convert-oc2dart"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.convert-oc2dart",
				"title": "convert Objective-C header to dart"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "yarn run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.25",
		"tslint": "^5.8.0",
		"typescript": "^3.1.4",
		"vscode": "^1.1.25"
	},
	"dependencies": {
		"oc2dart": "^0.0.3"
	}
}
